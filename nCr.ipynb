    "def C(n,r):\n",
    "    if(n == r or r == 0):\n",
    "        return 1\n",
    "    return C(n-1,r-1)+C(n-1,r)\n",
    "n = int(input())\n",
    "r = int(input())\n",
    "\n",
    "\n",
    "if(n<r or n<0 or r<0):\n",
    "    print(\"\\n\\n\\tInvalid\\n\\n\")\n",
    "\n",
    "elif(n == r or r == 0):\n",
    "    print(\"\\n\\n 1 \\n\\n\")\n",
    "else:\n",
    "    print(C(n,r))"
